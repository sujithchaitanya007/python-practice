x = 15  
print(x)  

x += 7  
print(x)  

x -= 4  
print(x)  

x *= 3  
print(x)  

x /= 5  
print(x)  

x %= 4  
print(x)  

x **= 3  
print(x)  

x //= 2  
print(x)  

a = 12 + 6  
print(a)  

b = 20 - 8  
print(b)  

c = 7 * 4  
print(c)  

d = 36 / 6  
print(d)  

e = 25 % 4  
print(e)  

f = 5 ** 3  
print(f)  

g = 17 // 3  
print(g)  
